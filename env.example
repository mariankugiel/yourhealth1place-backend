# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/yourhealth1place_db
DATABASE_TEST_URL=postgresql://username:password@localhost:5432/yourhealth1place_test_db

# Supabase Configuration (Auth, Personal Info, Lightweight Metadata)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# AWS Configuration (Sensitive Health Data)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=yourhealth1place-documents
AWS_DYNAMODB_TABLE=yourhealth1place-health-data
AWS_ATHENA_DATABASE=yourhealth1place_analytics
AWS_ATHENA_WORKGROUP=primary

# Akeyless Configuration (Encryption Keys)
AKEYLESS_ACCESS_ID=your-akeyless-access-id
AKEYLESS_ACCESS_KEY=your-akeyless-access-key
AKEYLESS_URL=https://api.akeyless.io

# Security Configuration
SECRET_KEY=your-secret-key-here-make-it-long-and-random
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password

# Application Configuration
DEBUG=True
ENVIRONMENT=development
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=YourHealth1Place API
VERSION=1.0.0 